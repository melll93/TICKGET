<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="back.spring.final_back.festival.repository.SearchDao">
	<select id="searchFestivalsByKeyword" resultType="FestivalDto">
SELECT * FROM fest_main WHERE fest_m_name LIKE #{keyword}
	</select>
	<select id="findAll" resultType="FestivalDto">
SELECT * FROM fest_main
	</select>
</mapper>


